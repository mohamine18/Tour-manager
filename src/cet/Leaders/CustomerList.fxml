<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="674.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="cet.Leaders.CustomerListController">
  <children>
    <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <ToolBar>
          <items>
            <TextField fx:id="TFSearch" onKeyTyped="#TFSearchName" prefWidth="290.0" promptText="Search Field" />
            <RadioButton fx:id="RBName" mnemonicParsing="false" selected="true" text="Search Name" />
            <RadioButton fx:id="RBPhone" mnemonicParsing="false" text="Search Phone" />
          </items>
          <VBox.margin>
            <Insets />
          </VBox.margin>
        </ToolBar>
        <TableView fx:id="CusTable">
          <columns>
            <TableColumn minWidth="50.0" prefWidth="218.0" text="Name" fx:id="CusNameCol" />
            <TableColumn prefWidth="50.0" text="Gender" fx:id="CusGenCol" />
            <TableColumn prefWidth="100.0" text="PPC &amp; CID" fx:id="CusCIDCol" />
            <TableColumn prefWidth="120.0" text="Phone" fx:id="CusPhoneCol" />
            <TableColumn prefWidth="110.0" text="Country" fx:id="CusCountryCol" />
            <TableColumn prefWidth="75.0" text="Date of Birth" fx:id="CusDOBCol" />
          </columns>
        </TableView>
        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0">
          <children>
            <Button id="BTSelect" mnemonicParsing="false" onAction="#SelectPressed" text="Select">
              <font>
                <Font size="13.0" fx:id="x1" />
              </font>
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="5.0" top="10.0" />
              </HBox.margin>
            </Button>
            <Button id="BTCancel" font="$x1" mnemonicParsing="false" onAction="#CancelPressed" text="Cancel">
              <HBox.margin>
                <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
              </HBox.margin>
            </Button>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</AnchorPane>

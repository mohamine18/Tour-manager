<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="cet.CashCollector.CollectorListController">
  <children>
    <VBox id="VBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <TableView fx:id="ColloctorTable">
          <columns>
            <TableColumn minWidth="100.0" prefWidth="200.0" text="Name" fx:id="NameCol" />
            <TableColumn minWidth="100.0" prefWidth="150.0" text="Adresse" fx:id="AdressCol" />
            <TableColumn minWidth="100.0" prefWidth="150.0" text="Phone" fx:id="PhoneCol" />
          </columns>
        </TableView>
        <HBox alignment="CENTER">
          <children>
            <Button fx:id="BTSelect" contentDisplay="CENTER" mnemonicParsing="false" onAction="#Select" text="Select">
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="5.0" top="5.0" />
              </HBox.margin>
            </Button>
            <Button fx:id="BTcancel" contentDisplay="CENTER" mnemonicParsing="false" onAction="#Cancel" text="Cancel">
              <HBox.margin>
                <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
              </HBox.margin>
            </Button>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</AnchorPane>
